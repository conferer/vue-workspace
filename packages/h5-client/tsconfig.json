{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "/src": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "element-plus/global",
      "@intlify/vite-plugin-vue-i18n/client"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
